<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
  <ShortName>Yahoo</ShortName>
  <Description>Yahoo! Search</Description>
  <InputEncoding>UTF-8</InputEncoding>
  <Image width="16" height="16">data:image/jpeg;base64,</Image>
  <Url type="application/x-suggestions+json" method="GET" template="http://hk-sayt.ff.search.yahoo.com/gossip-hk/?ei=utf-8&amp;output=fxjson&amp;command={searchTerms}" />
  <Url type="text/html" method="GET" template="http://hk.search.yahoo.com/search">
    <Param name="p" value="{searchTerms}"/>
    <Param name="ei" value="UTF-8"/>
    <MozParam name="fr" condition="pref" pref="yahoo-fr" />
  </Url>
  <SearchForm>http://hk.search.yahoo.com</SearchForm>
</SearchPlugin>
