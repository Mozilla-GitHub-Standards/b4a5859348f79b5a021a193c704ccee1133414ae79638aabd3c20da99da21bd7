            <Description>&yavideo.search.tooltip;</Description>
            <Url template="&yavideo.search.url;&amp;clid={clid6}" type="text/html"/>
            <Url rel="suggestions" template="&yavideo.suggestions.url;" type="application/json"/>
            <Url rel="moz-keywordsearch" template="&yavideo.search.url;&amp;clid={clid9}" type="application/x-moz-keywordsearch"/>
            <Image height="16" width="16">icons/yavideo.ico</Image>
            <InputEncoding>UTF-8</InputEncoding>
