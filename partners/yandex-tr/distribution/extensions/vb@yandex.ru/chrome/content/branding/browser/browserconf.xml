            <Url rel="moz-keywordsearch" template="&yagorsel.search.site-url;&amp;clid={clid9}" type="application/x-moz-keywordsearch"/>
            <Image height="16" width="16">icons/yagorsel.ico</Image>
            <InputEncoding>UTF-8</InputEncoding>
        </OpenSearchDescription>

        <OpenSearchDescription yb:uniqName="yavideo" xmlns="http://a9.com/-/spec/opensearch/1.1/">
