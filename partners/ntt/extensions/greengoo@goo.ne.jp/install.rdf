      <!-- Thunderbird -->
	  <RDF:Description em:id="{3550f703-e582-4d05-9a08-453d09bdfdc6}"
	                   em:minVersion="2.0"
	                   em:maxVersion="3.0.*" />
	</em:targetApplication>
  </RDF:Description>
</RDF:RDF>
