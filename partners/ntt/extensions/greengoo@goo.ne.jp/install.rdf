  <RDF:Description RDF:about="urn:mozilla:install-manifest">
    <em:id>greengoo@goo.ne.jp</em:id>
    <em:name>goo Green Label</em:name>
    <em:version>1.2.1</em:version>
    <em:description>Adds "goo tree" toolbar icon, the symbol of your contribution to environmental conservation. Moreover, your Firefox becomes green.</em:description>
    <em:creator>NTT Resonant Inc., ClearCode Inc.</em:creator>
    <em:homepageURL>http://green.goo.ne.jp/index_en</em:homepageURL>
    <em:optionsURL>chrome://greengoo/content/config.xul</em:optionsURL>
    <em:iconURL>chrome://greengoo/content/icon.png</em:iconURL>


    <em:localized>
      <RDF:Description em:locale="en-US">
        <em:name>goo Green Label</em:name>
        <em:description>Adds "goo tree" toolbar icon, the symbol of your contribution to environmental conservation. Moreover, your Firefox becomes green.</em:description>
        <em:creator>NTT Resonant Inc., ClearCode Inc.</em:creator>
        <em:homepageURL>http://green.goo.ne.jp/index_en</em:homepageURL>
      </RDF:Description>
    </em:localized>
    <em:localized>
      <RDF:Description em:locale="ja">
        <em:name>緑のgoo</em:name>
        <em:description>Adds "goo tree" toolbar icon, the symbol of your contribution to environmental conservation. Moreover, your Firefox becomes green.</em:description>
        <em:creator>NTTレゾナント株式会社, 株式会社クリアコード</em:creator>
        <em:homepageURL>http://green.goo.ne.jp/</em:homepageURL>
      </RDF:Description>
    </em:localized>


	<em:targetApplication>
