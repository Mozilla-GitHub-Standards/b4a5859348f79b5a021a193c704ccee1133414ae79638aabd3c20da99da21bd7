<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE widget SYSTEM "direct/entities.dtd"><widget bar-api="native" xmlns:f="http://bar.yandex.ru/dev/functional" icon="direct/content/icons/ico.png" icon-vector="direct/content/icons/ico.svg" name="&direct.name;" xmlns:ui="http://bar.yandex.ru/dev/gui" unique="true">

    <f:setting default="15" name="update-interval" scope="widget">
        <control empty-default="5" label="&direct.setting.update_interval;" label-after="&direct.setting.minutes;" max="10000" min="0" spin="5" type="textedit" value-type="int"/>
    </f:setting>

    <f:setting default="0" name="lastUpdate" scope="widget"/>

    <modules>
        <module browser="firefox" file="direct/core/widget.jsm"/>
    </modules>
</widget>
